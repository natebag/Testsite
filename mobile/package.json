{
  "name": "mlg-clan-mobile",
  "version": "1.0.0",
  "description": "MLG.clan Mobile Application - React Native gaming platform with blockchain integration",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace MLGClan.xcworkspace -scheme MLGClan -configuration Release -destination generic/platform=iOS",
    "bundle:android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle:ios": "npx react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle",
    "postinstall": "cd ios && pod install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.5",
    "@react-native-camera/camera": "^4.2.1",
    "@react-native-clipboard/clipboard": "^1.13.2",
    "@react-native-community/netinfo": "^10.0.0",
    "@react-native-firebase/app": "^18.7.3",
    "@react-native-firebase/messaging": "^18.7.3",
    "@react-native-firebase/analytics": "^18.7.3",
    "@react-native-masked-view/masked-view": "^0.3.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@reduxjs/toolkit": "^2.0.1",
    "@solana/wallet-adapter-phantom": "^0.9.24",
    "@solana/web3.js": "^1.87.6",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-background-job": "^1.2.2",
    "react-native-biometrics": "^3.0.1",
    "react-native-bootsplash": "^5.4.0",
    "react-native-device-info": "^10.12.0",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fs": "^2.20.0",
    "react-native-game-engine": "^1.2.0",
    "react-native-gamepad": "^1.0.3",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-image-picker": "^7.0.3",
    "react-native-keychain": "^8.1.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-lottie": "^6.4.1",
    "react-native-orientation-locker": "^1.5.0",
    "react-native-permissions": "^3.10.1",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "^3.6.1",
    "react-native-safe-area-context": "^4.8.0",
    "react-native-screens": "^3.27.0",
    "react-native-share": "^10.0.2",
    "react-native-sound": "^0.11.2",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-svg": "^14.0.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-video": "^5.2.1",
    "react-native-webview": "^13.6.4",
    "react-redux": "^9.0.4",
    "redux-persist": "^6.0.0",
    "redux-persist-encrypted-storage": "^2.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/runtime": "^7.23.5",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.12",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.38",
    "@types/react-native": "^0.72.6",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-jest": "^29.7.0",
    "detox": "^20.13.5",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^3.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.2"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "device": {
          "type": "ios.simulator",
          "device": {
            "type": "iPhone 14"
          }
        },
        "app": {
          "type": "ios.app",
          "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/MLGClan.app"
        }
      },
      "android.emu.debug": {
        "device": {
          "type": "android.emulator",
          "device": {
            "avdName": "Pixel_6_API_33"
          }
        },
        "app": {
          "type": "android.apk",
          "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk"
        }
      }
    },
    "testRunner": {
      "args": {
        "$0": "jest",
        "config": "e2e/jest.config.js"
      }
    }
  },
  "keywords": [
    "react-native",
    "gaming",
    "blockchain",
    "solana",
    "mobile",
    "clan",
    "mlg"
  ],
  "author": "MLG.clan Development Team",
  "license": "MIT"
}