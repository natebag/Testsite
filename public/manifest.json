  "share_target": {
    "action": "/pages/content.html?share=true",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "body",
      "url": "url",
      "files": [
        {
          "name": "media",
          "accept": [
            "image/png",
            "image/jpeg", 
            "image/gif",
            "image/webp",
            "video/mp4",
            "video/webm",
            "video/quicktime"
          ]
        },
        {
          "name": "gaming_clips",
          "accept": [
            "video/mp4",
            "video/webm"
          ]
        }
      ]
    }
  },
  
  "file_handlers": [
    {
      "action": "/pages/content.html?handler=gaming_clip",
      "accept": {
        "video/mp4": [".mp4"],
        "video/webm": [".webm"],
        "video/quicktime": [".mov"]
      },
      "icons": [
        {
          "src": "/assets/icons/video-handler.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ],
      "launch_type": "single-client"
    },
    {
      "action": "/pages/profile.html?handler=gaming_stats",
      "accept": {
        "application/json": [".json"],
        "text/csv": [".csv"]
      },
      "icons": [
        {
          "src": "/assets/icons/stats-handler.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ],
      "launch_type": "single-client"
    }
  ],
  
  "protocol_handlers": [
    {
      "protocol": "web+mlgclan",
      "url": "/pages/index.html?action=%s"
    },
    {
      "protocol": "web+gaming",
      "url": "/pages/content.html?gaming=%s"
    },
    {
      "protocol": "web+clan",
      "url": "/pages/clans.html?invite=%s"
    },
    {
      "protocol": "web+vote",
      "url": "/pages/voting.html?content=%s"
    }
  ],
  
  "widgets": [
    {
      "name": "Gaming Stats",
      "short_name": "Stats",
      "description": "Quick view of your gaming statistics and achievements",
      "tag": "gaming_stats",
      "template": "gaming_stats_template",
      "ms_ac_template": "gaming_stats_adaptive_card.json",
      "data": "/api/widgets/gaming-stats",
      "type": "application/json",
      "auth": false,
      "update": 300,
      "icons": [
        {
          "src": "/assets/icons/stats-widget.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ],
      "screenshots": [
        {
          "src": "/assets/screenshots/stats-widget.png",
          "sizes": "300x200",
          "type": "image/png",
          "label": "Gaming Statistics Widget"
        }
      ]
    },
    {
      "name": "Clan Activity",
      "short_name": "Clan",
      "description": "Latest clan member activities and achievements",
      "tag": "clan_activity",
      "template": "clan_activity_template",
      "ms_ac_template": "clan_activity_adaptive_card.json",
      "data": "/api/widgets/clan-activity",
      "type": "application/json",
      "auth": true,
      "update": 180,
      "icons": [
        {
          "src": "/assets/icons/clan-widget.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ],
      "screenshots": [
        {
          "src": "/assets/screenshots/clan-widget.png",
          "sizes": "300x200",
          "type": "image/png",
          "label": "Clan Activity Widget"
        }
      ]
    },
    {
      "name": "Quick Vote",
      "short_name": "Vote",
      "description": "Instantly vote on trending gaming content",
      "tag": "quick_vote",
      "template": "quick_vote_template",
      "ms_ac_template": "quick_vote_adaptive_card.json",
      "data": "/api/widgets/trending-content",
      "type": "application/json",
      "auth": true,
      "update": 120,
      "icons": [
        {
          "src": "/assets/icons/vote-widget.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ],
      "screenshots": [
        {
          "src": "/assets/screenshots/vote-widget.png",
          "sizes": "300x200",
          "type": "image/png",
          "label": "Quick Vote Widget"
        }
      ]
    }
  ],
  
  "scope_extensions": [
    {
      "origin": "*.mlgclan.com"
    },
    {
      "origin": "*.solana.com",
      "reason": "Solana blockchain integration for gaming tokens"
    },
    {
      "origin": "*.phantom.app",
      "reason": "Phantom wallet integration for Web3 gaming"
    }
  ],
  
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.mlgclan.app",
      "id": "com.mlgclan.app",
      "min_version": "1.0.0"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/app/mlgclan/id123456789",
      "id": "123456789"
    },
    {
      "platform": "windows",
      "url": "https://www.microsoft.com/store/apps/9NBLGGH5L9XT",
      "id": "9NBLGGH5L9XT"
    },
    {
      "platform": "chrome_web_store",
      "url": "https://chrome.google.com/webstore/detail/mlgclan/abcdefghijklmnopqrstuvwxyz",
      "id": "abcdefghijklmnopqrstuvwxyz"
    }
  ],
  
  "prefer_related_applications": false,
  
  "edge_side_panel": {
    "preferred_width": 400
  },
  
  "web_accessible_resources": [
    {
      "resources": [
        "assets/icons/*",
        "assets/sounds/*",
        "assets/animations/*"
      ],
      "matches": ["*"]
    }
  ],
  
  "permissions": [
    "camera",
    "microphone",
    "notifications",
    "background-sync",
    "clipboard-read",
    "clipboard-write"
  ],
  
  "features": [
    "Cross-platform gaming integration",
    "Web3 blockchain rewards",
    "Real-time clan management",
    "Token-powered voting system",
    "Offline gaming statistics",
    "Push notifications for tournaments",
    "Native mobile sharing",
    "Camera integration for gaming content",
    "QR code clan invitations",
    "Haptic feedback for gaming interactions",
    "Battery optimization for extended sessions",
    "Gaming-optimized performance"
  ]
}