{
  "name": "mlg-clan-platform",
  "version": "1.0.0",
  "description": "MLG.clan gaming platform with blockchain integration and comprehensive authentication",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:auth": "jest --config jest.config.cjs src/auth/auth.test.js",
    "test:integration": "node run-integration-tests.js",
    "test:headless": "HEADLESS=true node run-integration-tests.js",
    "test:ci": "npm install && npm run test && npm run test:headless",
    "test:cors": "node test-cors-configuration.js",
    "start:frontend": "node temp-server.js",
    "start:demo": "cd demo && node server.js",
    "serve": "python -m http.server 8080",
    "db:migrate": "node src/database/migrations/migration-runner.js",
    "db:seed": "node src/database/seeds/development-seed.sql"
  },
  "dependencies": {
    "@solana/wallet-adapter-phantom": "^0.9.24",
    "@solana/web3.js": "^1.87.6",
    "bcryptjs": "^2.4.3",
    "bs58": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "isomorphic-dompurify": "^2.26.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "puppeteer": "^21.5.2",
    "qrcode": "^1.5.3",
    "redis": "^4.6.12",
    "speakeasy": "^2.0.0",
    "tweetnacl": "^1.0.3",
    "uuid": "^9.0.1",
    "validator": "^13.15.15",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "testing",
    "integration",
    "puppeteer",
    "web3",
    "gaming"
  ],
  "author": "MLG.clan Development Team",
  "license": "MIT"
}
